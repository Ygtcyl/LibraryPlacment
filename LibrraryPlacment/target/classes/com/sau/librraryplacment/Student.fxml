<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="180.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sau.librraryplacment.controller.StudentController">
        <children>
            <Label layoutX="26.0" layoutY="20.0" text="ID:" />
            <Label layoutX="26.0" layoutY="50.0" text="NAME:" />
            <Label layoutX="26.0" layoutY="79.0" text="DPRTMNT" />
            <Label layoutX="26.0" layoutY="109.0" />
            <TextField fx:id="studentId" layoutX="89.0" layoutY="16.0" />
            <TextField fx:id="name" layoutX="89.0" layoutY="46.0" />
            <TextField fx:id="dprtmnt" layoutX="89.0" layoutY="76.0" />
            <Button fx:id="getStudent" layoutX="267.0" layoutY="16.0" mnemonicParsing="false" onAction="#getStudent" text="GET" />
            <Button fx:id="updateStudent" layoutX="96.0" layoutY="138.0" mnemonicParsing="false" onAction="#updateStudent" text="UPDATE" />
            <Button fx:id="saveStudent" layoutX="34.0" layoutY="139.0" mnemonicParsing="false" onAction="#saveStudent" text="SAVE" />
            <Button fx:id="deleteStudent" layoutX="178.0" layoutY="138.0" mnemonicParsing="false" onAction="#deleteStudent" text="DELETE" />
            <Button fx:id="close" layoutX="259.0" layoutY="137.0" mnemonicParsing="false" onAction="#close" text="CLOSE" />
            <Button fx:id="clearStudent" layoutX="261.0" layoutY="77.0" mnemonicParsing="false" onAction="#clearStudent" text="CLEAR" />
        </children>
</AnchorPane>
