        <TextField fx:id="name" layoutX="89.0" layoutY="46.0" />
        <TextField fx:id="address" layoutX="89.0" layoutY="76.0" />
        <Button fx:id="getPlace" layoutX="267.0" layoutY="16.0" mnemonicParsing="false" onAction="#getPlace" text="GET" />
