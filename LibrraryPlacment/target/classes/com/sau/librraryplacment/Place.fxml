<?import javafx.scene.layout.AnchorPane?>



<AnchorPane prefHeight="180.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sau.librraryplacment.controller.PlaceController">
    <children>
        <Label layoutX="26.0" layoutY="20.0" text="ID:" />
